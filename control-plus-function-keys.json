{
  "title": "Function Keys & Media Control",
  "rules": [
    {
      "description": "Set F1, F2, F3... to F1, F2, F3...",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f1"
          },
          "to": [
            {
              "key_code": "f1"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f2"
          },
          "to": [
            {
              "key_code": "f2"
            }
          ]
        },
        {
