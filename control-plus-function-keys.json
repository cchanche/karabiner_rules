          },
          "to": [
            {
              "key_code": "f12"
            }
          ]
        }
      ]
    },
    {
      "description": "Control + F10, F11, F12 to media",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f11",
            "modifiers": {
              "mandatory": ["left_control"]
            }
          },
          "to": [
            {
              "key_code": "play_or_pause"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f12",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "consumer_key_code": "scan_next_track"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f10",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "consumer_key_code": "scan_previous_track"
            }
          ]
        }
      ]
    }
  ]
